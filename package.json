  "description": "remix server: allow accessing file system from remix.ethereum.org and start a dev environment (see help section)",
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "bin": {
    "remixd": "./lib/bin/remixd.js"
  },
  "scripts": {

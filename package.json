  },
  "homepage": "https://github.com/ethereum/remixd#readme",
  "dependencies": {
    "@remixproject/plugin": "0.3.0-alpha.3",
    "@remixproject/plugin-ws": "0.3.0-alpha.1",
    "chalk": "^4.0.0",
    "chokidar": "^2.0.2",
    "commander": "^2.20.3",
    "fs-extra": "^3.0.1",
    "isbinaryfile": "^3.0.2",
    "ws": "^7.3.0"
  },
  "python": {
